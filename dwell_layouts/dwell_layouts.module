<?php

/**
 * @file
 * Contains dwell_layouts.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function dwell_layouts_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the dwell_layouts module.
    case 'help.page.dwell_layouts':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Custom layouts used in Manage Display') . '</p>';
      return $output;

    default:
  }
}
